# looking at yearly ranking(s) of South African universities using the QS World University Rankings
#defaultView:LineChart
# to add another university :
# to add another deternimationMethod :
SELECT DISTINCT ?year ?ranking ?universityLabel ?deternimationMethodLabel ?university WHERE {
     VALUES ?university {
    wd:Q534643
    wd:Q951305
    wd:Q1972050
    wd:Q1066492
    }
  VALUES ?deternimationMethod {
    wd:Q1790510
  } 
 ?university p:P1352 ?statement.
  ?statement ps:P1352 ?ranking;
    pq:P459 ?deternimationMethod;
    pq:P585 ?date.
  BIND(STR(YEAR(?date)) AS ?year)
  SERVICE wikibase:label { bd:serviceParam wikibase:language "[AUTO_LANGUAGE],mul,en". }
}
